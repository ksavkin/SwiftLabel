# SwiftLabel Environment Variables
# Copy this file to .env and modify as needed

# Server bind address (default: 127.0.0.1)
# WARNING: Never bind to 0.0.0.0 - SwiftLabel is designed for local use only
SWIFTLABEL_HOST=127.0.0.1

# Server port (default: 8765)
SWIFTLABEL_PORT=8765

# Enable debug logging (default: false)
SWIFTLABEL_DEBUG=false
